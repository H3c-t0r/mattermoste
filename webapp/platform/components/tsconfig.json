{
    "compilerOptions": {
        "target": "esnext",
        "module": "esnext",
        "moduleResolution": "node",
        "jsx": "react",
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "skipLibCheck": true,
        "strictNullChecks": true,
        "isolatedModules": true,
        "noEmit": true,
        "declaration": true,
        "outDir": "dist"
    }
}
